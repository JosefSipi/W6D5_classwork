<h1><%= @cat.name %>'s Profile</h1>

<table>
    <tr>
        <th colspan="2"> <% @cat.name%> </th>
    </tr>
    <tr>
        <th> Attribute </th>
        <th> Value </th>
    </tr>
    <tr>
        <td> Name: </td>
        <td> <%=@cat.name%> </td>
    </tr>
    <tr>
        <td> Birth Day: </td>
        <td> <%=@cat.birth_date%> </td>
    </tr>
    <tr>
        <td> Color: </td>
        <td> <%=@cat.color%> </td>
    </tr>
    <tr>
        <td> Description: </td>
        <td> <%=@cat.description%> </td>
    </tr>
    <tr>
        <td> Sex: </td>
        <td> <%=@cat.sex%> </td>
    </tr>

</table>
